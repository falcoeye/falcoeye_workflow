{
    "KAUST Fish Counter":{
        "name": "KAUST Fish Counter",
        "model": "KAUST Fish Finder",
        "type": "ObjectDetection",
        "framework": "tensorflow",
        "input_args": [
            {
                "name": "output_path",
                "type": "str",
                "required": true
            },
            {
                "name": "source_type",
                "type": "str",
                "default": "video"
            }
        ],
        "calculations": [
            {
                "type": "record_timestamp",
                "name": "timestamp",
                "source": "$source_type",
                "on": "prediction"
            },
            {
                "type": "type_counter",
                "class": "fish",
                "name": "fish_count_per_frame",
                "on": "prediction"
            },
            {
                "type": "put_on_pd",
                "calculations": ["timestamp","fish_count_per_frame"],
                "name": "fish_count",
                "on": "calculation"
            }
        ],
        "outputters":[
            {
                "name": "fish_count_outputter",
                "io_handler": "csv",
                "on": "calculation",
                "calculation": "fish_count",
                "output_path": "$output_path"
            }
        ]
    }
}
