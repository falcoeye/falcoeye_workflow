{
    "KAUST Fish Counter":{
        "name": "KAUST Fish Counter",
        "model": "KAUST Fish Finder",
        "type": "ObjectDetection",
        "framework": "tensorflow",
        "input_args": [
            {
                "name": "output_path",
                "type": "str",
                "required": true,
                "source": "system"
            },
            {
                "name": "source_type",
                "type": "str",
                "default": "video",
                "source": "system"
            }
        ],
        "calculations": [
            {
                "type": "record_timestamp",
                "name": "timestamp",
                "source": "$source_type",
                "on": "prediction"
            },
            {
                "type": "type_counter",
                "class": "fish",
                "name": "fish_count_per_frame",
                "on": "prediction"
            },
            {
                "type": "put_on_pd",
                "calculations": ["timestamp","fish_count_per_frame"],
                "name": "fish_count",
                "on": "calculation"
            }
        ],
        "outputters":[
            {
                "name": "fish_count_outputter",
                "io_handler": "csv",
                "on": "calculation",
                "calculation": "fish_count",
                "output_path": "$output_path"
            }
        ]
    },
    "Vehicles Counter":{
        "name": "Vehicles Counter",
        "model": "CarEye",
        "type": "ObjectDetection",
        "framework": "torch",
        "input_args": [
            {
                "name": "output_path",
                "type": "str",
                "required": true,
                "source": "system"
            },
            {
                "name": "source_type",
                "type": "str",
                "default": "video",
                "source": "system"
            }
        ],
        "calculations": [
            {
                "type": "record_timestamp",
                "name": "timestamp",
                "source": "$source_type",
                "on": "prediction"
            },
            {
                "type": "type_counter",
                "class": [
                    "two-wheels",
                    "four-wheels",
                    "bus",
                    "truck"],
                "name": "veh_count_per_frame",
                "on": "prediction"
            },
            {
                "type": "put_on_pd",
                "calculations": ["timestamp","veh_count_per_frame"],
                "name": "veh_count",
                "on": "calculation"
            }
        ],
        "outputters":[
            {
                "name": "veh_count_outputter",
                "io_handler": "csv",
                "on": "calculation",
                "calculation": "veh_count",
                "output_path": "$output_path"
            }
        ]
    },
    "Human Counter":{
        "name": "Human Counter",
        "model": "Human Counter",
        "type": "ObjectDetection",
        "framework": "tensorflow",
        "input_args": [
            {
                "name": "output_path",
                "type": "str",
                "required": true,
                "source": "system"
            },
            {
                "name": "source_type",
                "type": "str",
                "default": "video",
                "source": "system"
            }
        ],
        "calculations": [
            {
                "type": "record_timestamp",
                "name": "timestamp",
                "source": "$source_type",
                "on": "prediction"
            },
            {
                "type": "type_counter",
                "class": "person",
                "name": "human_count_per_frame",
                "on": "prediction"
            },
            {
                "type": "put_on_pd",
                "calculations": ["timestamp","human_count_per_frame"],
                "name": "human_count",
                "on": "calculation"
            }
        ],
        "outputters":[
            {
                "name": "human_count_outputter",
                "io_handler": "csv",
